{
  "name": "FacebookFilter",
  "version": "0.1",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": [
        "https://www.facebook.com/",
        "http://www.facebook.com/"
      ],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "default.png",
    "48": "default.png",
    "128": "default.png"
  },
  "page_action": {
    "default_icon": {
      "16": "default.png",
      "48": "default.png",
      "128": "default.png"
    }, 
    "default_title": "FacebookFilter"
  }
}
